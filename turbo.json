{
  "$schema": "https://turbo.build/schema.json",
  "globalDotEnv": [
    ".env"
  ],
  "globalPassThroughEnv": [
    "CI"
  ],
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ],
      "env": [
        "VERCEL_URL"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "dotEnv": [
        ".env"
      ],
      "env": [
        "NEXT_PUBLIC_SUPABASE_URL",
        "NEXT_PUBLIC_SUPABASE_ANON_KEY",
        "SUPABASE_SERVICE_KEY",
        "EDGE_CONFIG",
        "VERCEL_ENV",
        "FLAGS_SECRET"
      ]
    },
    "supabase#start": {
      "dependsOn": [
        "^start"
      ],
      "cache": false
    },
    "supabase#stop": {
      "dependsOn": [
        "^stop"
      ],
      "cache": false
    },
    "supabase#generate-types": {
      "dependsOn": [
        "^generate-types"
      ],
      "cache": false
    },
    "supabase#build": {
      "dependsOn": [
        "supabase#generate-types"
      ],
      "cache": false
    },
    "test": {
      "dependsOn": [
        "^test"
      ],
      "cache": false,
      "env": [
        "NEXT_PUBLIC_SUPABASE_URL",
        "NEXT_PUBLIC_SUPABASE_ANON_KEY",
        "SUPABASE_SERVICE_KEY",
        "NEXT_PUBLIC_BASE_URL"
      ]
    },
    "test:ui": {
      "dependsOn": [
        "^test:ui"
      ],
      "cache":false,
      "env": [
        "NEXT_PUBLIC_SUPABASE_URL",
        "NEXT_PUBLIC_SUPABASE_ANON_KEY",
        "SUPABASE_SERVICE_KEY",
        "NEXT_PUBLIC_BASE_URL"
      ]
    },
    "rental-review#start": {
      "dependsOn": [
        "rental-review#build"
      ],
      "env": [
        "NEXT_PUBLIC_SUPABASE_URL",
        "NEXT_PUBLIC_SUPABASE_ANON_KEY",
        "SUPABASE_SERVICE_KEY",
        "EDGE_CONFIG",
        "VERCEL_ENV",
        "FLAGS_SECRET"
      ],
      "cache": false,
      "persistent": true
    }
  }
}